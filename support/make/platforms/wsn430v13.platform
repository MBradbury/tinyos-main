PFLAGS += -I%T/lib/power

PFLAGS += -I%T/platforms/wsn430v13/chips/cc1101
PFLAGS += -I%T/platforms/wsn430v13/chips/cc1101/alarm
PFLAGS += -I%T/platforms/wsn430v13/chips/cc1101/control
PFLAGS += -I%T/platforms/wsn430v13/chips/cc1101/csma
PFLAGS += -I%T/platforms/wsn430v13/chips/cc1101/interfaces
